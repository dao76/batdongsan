                            window.location.href = `http://localhost:3000/admin/table/news/${id}/edit`
                        }
                        let deleted_id;
                        const onClickDelete = (id) => {
                            deleted_id = id;
                        }
                        const fetchAPI = async (url, option) => {
                            const res = await fetch(url, option);
                            return res.json();
                        }
                        const onDelete = async () => {
                            let url = `http://localhost:3000/admin/table/news/${deleted_id}/delete`;
                            const option = {
                                method: 'delete',
                                headers: { 'Content-Type': 'application/json' }
                            }
                            try {
                                await fetchAPI(url, option);
